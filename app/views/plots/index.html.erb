<h3>Plots:</h3>
<div id="plots">
<% @plots.each do |plot| %>
    <div class="plot">
        <p><%= plot.number %></p>
        <% plot.plants.each do |plant| %>
         <p><%= plant.name %></p>
         <p><%= button_to "Delete #{plant.name}", "/plant_plots/", method: :delete, params: { plant_id: plant.id, plot_id: plot.id } %> </p>
        <% end %>
    </div>
<% end %>
</div>